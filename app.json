{
  "expo": {
    "name": "battle-bets",
    "slug": "battle-bets",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon_v2.png",
    "scheme": "battlebets",          
    "deepLinking": true,
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash_v1.png",
      "resizeMode": "contain",
      "backgroundColor": "#061826"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.battlebets.battlebets",
      "buildNumber": "2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bfe2efbe-b772-4641-aa87-a4fe52c42ef0"
      }
    }
  }
}
