{
  "expo": {
    "name": "Battle Bets",
    "slug": "battle-bets",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/iOS-icon.png",
    "scheme": "battlebets",
    "deepLinking": true,
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#061826"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.battlebets.battlebets",
      "buildNumber": "4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:aasa.battlebets.app",
        "applinks:join.battlebets.app"
      ]
    },
    "android": {
      "package": "com.battlebets.battlebets",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "join.battlebets.app",
              "pathPrefix": "/join"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bfe2efbe-b772-4641-aa87-a4fe52c42ef0"
      }
    }
  }
}
