{
  "cli": {
    "version": ">= 13.2.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ENV_FILE": ".env.development"
      }
    },
    "preview": {
      "environment": "preview",
      "autoIncrement": true,
      "distribution": "store"
    },
    "production": {
      "environment": "production",
      "autoIncrement": true,
      "distribution": "store"
    }
  },
  "submit": {
    "production": {}
  }
}